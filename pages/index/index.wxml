        商 务 活 动 篇：
    </view>
</view>
<swiper autoplay indicator-dots style="height:300rpx;width:100%;background:gray;filter:blur(0.4px)">
    <swiper-item style="text-align:center" wx:for="{{url}}" bindtap='getlocation' data-e="{{url}}">
        <image style='height:300rpx;width:80%;border-radius: 6rpx;box-shadow: 4rpx 8rpx 10rpx gray;' src="{{item}}" />
    </swiper-item>
</swiper>

<!--商务策划篇 -->
<view>
