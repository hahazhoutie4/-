    <view style="width:136rpx;height:136rpx;background:purple;border-radius:68rpx">
        <view style="width:120rpx;height:120rpx;background:white;margin:0 auto;line-height:120rpx;border-radius:60rpx;text-align:center;">
            <text style='margin:0 auto;'>专项策划</text>
        </view>
    </view>
</view>

